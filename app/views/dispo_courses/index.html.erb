<p id="notice"><%= notice %></p>

<h1>Dispo Courses</h1>

<table>
  <thead>
    <tr>
      <th>Course</th>
      <th>Date</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @dispo_courses.each do |dispo_course| %>
      <tr>
        <td><%= dispo_course.course_id %></td>
        <td><%= dispo_course.date %></td>
        <td><%= link_to 'Show', dispo_course %></td>
        <td><%= link_to 'Edit', edit_dispo_course_path(dispo_course) %></td>
        <td><%= link_to 'Destroy', dispo_course, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Dispo Course', new_dispo_course_path %>
