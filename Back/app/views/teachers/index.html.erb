<p id="notice"><%= notice %></p>

<h1>Teachers</h1>

<table>
  <thead>
    <tr>
      <th>User</th>
      <th>Integer</th>
      <th>Theme</th>
      <th>Integer</th>
      <th>Title</th>
      <th>String</th>
      <th>Description</th>
      <th>String</th>
      <th>Cost</th>
      <th>Integer</th>
      <th>Photo</th>
      <th>String</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @teachers.each do |teacher| %>
      <tr>
        <td><%= teacher.user_id %></td>
        <td><%= teacher.integer %></td>
        <td><%= teacher.theme_id %></td>
        <td><%= teacher.integer %></td>
        <td><%= teacher.title %></td>
        <td><%= teacher.string %></td>
        <td><%= teacher.description %></td>
        <td><%= teacher.string %></td>
        <td><%= teacher.cost %></td>
        <td><%= teacher.integer %></td>
        <td><%= teacher.photo %></td>
        <td><%= teacher.string %></td>
        <td><%= link_to 'Show', teacher %></td>
        <td><%= link_to 'Edit', edit_teacher_path(teacher) %></td>
        <td><%= link_to 'Destroy', teacher, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

